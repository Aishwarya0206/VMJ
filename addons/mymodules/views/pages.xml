    <template id="academics_page_template">
        <t t-call="website.layout">
            <div id="wrap">
                <div class="container">
                    <h1>Academics</h1>
                    <ul class="academics">
                        <li>James Doak Hall</li>
                        <li>Wallace</li>
                        <li>Paulinder Hall</li>
                    </ul>

                    <!-- === Snippets' area === -->
                    <div class="oe_structure" />
                </div>
            </div>
        </t>
    </template>
    <record id="academics_page" model="website.page">
        <field name="name">Academics page</field>
        <field name="website_published">True</field>
        <field name="url">/academics</field>
        <field name="view_id" ref="academics_page_template"/>
    </record>
    <record id="academics_page_link" model="website.menu">
  	<field name="name">Academics</field>
  	<field name="page_id" ref="academics_page"/>
  	<field name="parent_id" ref="website.main_menu" />
  	<field name="sequence" type="int">99</field>
    </record>

